<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controllers.ConnecterAppController">
   <children>
      <Label fx:id="bienvenuelabel" layoutX="300.0" layoutY="30.0" text="Bienvenue">
         <font>
            <Font name="Britannic Bold" size="40.0" />
         </font>
      </Label>
      <Label fx:id="choixlabel" layoutX="300.0" layoutY="120.0" text="Choisisser Votre role">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="adminButton" layoutX="330.0" layoutY="200.0" mnemonicParsing="false" onAction="#AdminConnectButton" text="Administrateur" />
      <Button fx:id="userButton" layoutX="350.0" layoutY="300.0" mnemonicParsing="false" onAction="#UserConnectButton" text="Utilisateur" />
   </children>
</AnchorPane>
